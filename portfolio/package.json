    "style-min": "npm run style-light-min && npm run style-dark-min",
    "style-dark": "sass themes/super_theme/sass/style-dark.scss themes/super_theme/css/style-dark.css --no-source-map",
    "style-dark-min": "sass themes/super_theme/sass/style-dark.scss themes/super_theme/css/style-dark.min.css --no-source-map --style compressed",
    "style-light": "sass themes/super_theme/sass/style.scss themes/super_theme/css/style.css --no-source-map",
    "style-light-min": "sass themes/super_theme/sass/style.scss themes/super_theme/css/style.min.css --no-source-map --style compressed"
