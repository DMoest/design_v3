    "watch-sass": "sass themes/super_theme/sass/style.scss:themes/super_theme/css/style.css themes/super_theme/sass/style-dark.scss:themes/super_theme/css/style-dark.css --no-source-map --watch",
    "watch-sass-min": "sass themes/super_theme/sass/style.scss:themes/super_theme/css/style.min.css themes/super_theme/sass/style-dark.scss:themes/super_theme/css/style-dark.min.css --no-source-map --watch --style compressed",
